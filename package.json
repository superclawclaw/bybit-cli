{
  "name": "bybit-cli",
  "version": "0.1.0",
  "description": "Command-line interface for Bybit exchange with multi-account management, real-time monitoring, and full trading support",
  "type": "module",
  "bin": {
    "bb": "./dist/index.js"
  },
  "files": [
    "dist",
    "README.md",
    "SKILLS.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "node dist/index.js",
    "prepublishOnly": "pnpm build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/superclawclaw/bybit-cli.git"
  },
  "homepage": "https://github.com/bybit-cli/bybit-cli#readme",
  "bugs": {
    "url": "https://github.com/bybit-cli/bybit-cli/issues"
  },
  "keywords": [
    "bybit",
    "cli",
    "trading",
    "crypto",
    "cryptocurrency",
    "perpetual",
    "futures",
    "dex",
    "terminal",
    "tui"
  ],
  "author": "Derek",
  "dependencies": {
    "@inquirer/prompts": "^8.2.1",
    "better-sqlite3": "^11.8.1",
    "bybit-api": "^4.5.3",
    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^13.1.0",
    "ink": "^5.1.0",
    "ink-spinner": "^5.0.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.13.4",
    "@types/react": "^18.3.18",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "license": "MIT",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "esbuild"
    ]
  }
}
